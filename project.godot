[autoload]

PointsManager="*res://scripts/points_manager.gd"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="ignore"
mouse_cursor/custom_image="res://mouse_icon.png"

[rendering]

environment/defaults/default_clear_color=Color(0, 0, 0, 1)
